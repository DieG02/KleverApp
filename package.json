{
  "name": "KleverApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "cd android && ./gradlew installDevDebug && cd ..",
    "android:stage": "cd android && ./gradlew installProdDebug && cd ..",
    "android:prod": "cd android && ./gradlew installProdRelease && cd ..",
    "build:dev": "NODE_ENV=development npx react-native run-android --mode=devDebug",
    "build:stage": "NODE_ENV=production npx react-native run-android --mode=prodDebug",
    "build:prod": "NODE_ENV=production npx react-native run-android --mode=prodRelease",
    "clean": "cd android && ./gradlew clean && cd ..",
    "publish": "cd android && ./gradlew bundleProdRelease && cd ..",
    "cloud:dev": "firebase use dev && firebase deploy --only functions",
    "cloud:proud": "firebase use prod && firebase deploy --only functions",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/app": "^23.1.1",
    "@react-native-firebase/auth": "^23.1.1",
    "@react-native-firebase/firestore": "^23.1.1",
    "@react-native-firebase/functions": "^23.1.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-voice/voice": "^3.1.5",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "i18next": "^25.4.0",
    "react": "19.1.0",
    "react-i18next": "^15.7.1",
    "react-native": "0.80.2",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-localize": "^3.5.2",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.2",
    "react-native-svg": "^15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-toast-message": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "^0.80.2",
    "@react-native/codegen": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/gradle-plugin": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-jest": "^30.1.2",
    "cross-env": "^10.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
